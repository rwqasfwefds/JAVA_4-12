package ex3;

public class Excercise03 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
//		해당 클래스를 생성하고 인스턴스 메소드를 호출해서 아래를 출력하셈
//		클래스명 : Song
//		인스턴스 변수 : title = "Dancing Queen"
//			 singer = "ABBA"
//			 country = "스웨덴"
//		인스턴스 메소드 : show
//		출력 : "스웨덴 국적의 ABBA가 부른 Dancing Queen"
		Song sing = new Song();
		
		sing.show("Dancing Queen", "ABBA", "스웨덴");
	}

}
