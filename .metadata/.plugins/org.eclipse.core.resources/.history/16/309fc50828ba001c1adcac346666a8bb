package ex03;

// 클래스 설명 : 사칙연산을 위한 클래스
public class MyMath {
//	멤버 메소드(인스턴스 메소드), 일반 메소드
	long add(long a, long b) {
		long result = a + b;
		return result;
	}
	long subtract(long a, long b) {
		return a - b;
	}
	long multiply(long a, long b) {
		return a * b;
	}
	double divide(double a, double b) {
		return a / b;
	}
}
